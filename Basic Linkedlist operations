class Node:
    def __init__(self,data = None):
        self.data  = data
        self.next = None
        
class Linkedlist:
    def __init__(self):
        self.head = None
        self.tail = None
        self.size = 0
        
    def addLast(self,newdata):
        NewNode = Node(newdata)
        if self.head is None:
            self.head = NewNode
            self.tail = NewNode
            self.size +=1
        
        last = self.tail 
        last.next = NewNode
        self.tail = last.next
        self.size +=1
        
        
    def display(self):
        temp = self.head
        while temp is not None:
            print(temp.data ,end = ' ')
        temp = temp.next
        
        
    def removeFirst(self):
        if self.size == 0:
            print('List is empty')
            return
        
        self.head = self.head.next
        self.size -=1
        
        
